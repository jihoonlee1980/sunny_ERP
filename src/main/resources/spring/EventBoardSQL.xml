<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com">
	<select id="eventBoardCount" resultType="int">
		select count(*) from eventboard
	</select>
	<select id="eventBoardList" resultType="eventBoardDTO" parameterType="HashMap">
		select * from eventboard order by writeday asc limit #{start} , #{perpage}
	</select>
</mapper>